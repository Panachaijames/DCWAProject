<head>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.0-beta1/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-giJF6kkoqNQ00vy+HMDP7azOuL0xtbfIcaT9wjKHr8RbDVddVHyTfAAsrekwKmP1" crossorigin="anonymous">
    <style>
        h1{
            color:rgb(136, 98, 243);
        }
        table,
        th,
        td {
            border: 1px solid black;
        }

        a{
            text-decoration: none;
            color: seashell;
        }
        
    </style>
</head>

<body>
    <h1>Head Of State</h1>

    <button class="btn btn-secondary"><a href="/addHeadOfState">Add Head Of State</a> </button>
    <br><br><br>

    <table>
        <tr>
            <th>Country Code</th>
            <th>Head Of State</th>
        </tr>
        <% headsOfStates.forEach((headsOfState)=>
            { %>
            <tr>
                <td>
                    <%= headsOfState._id %>
                </td>
                <td>
                    <%= headsOfState.headOfState %>
                </td>
            </tr>
            <% }) %>
    </table>
        <br><br>
        <br><br>
        <br><br>   
        <button class="btn btn-secondary"><a href="/">&#60;&#60;HOME</a> </button>
</body>

</html>